  "license": "AGPL-3.0",
  "description": "### WIP. NOT IN PRODUCTION, TESTED AND/OR BENCHMARKED YET! install this package to get started with grundstein. npm i -g grundstein",
  "main": "src/index.mjs",
  "private": true,
  "engines": {
    "node": ">=13.5.0"
  },
  "engineStrict": true,
  "scripts": {
    "start": "npm run build && npm run docker && npm run dev",
    "build": "src/bin.mjs",
    "dev": "/usr/bin/env bash bootstrap/dev.sh",
    "docker": "/usr/bin/env bash bootstrap/docker.sh",
    "connect": "/usr/bin/env docker exec -it grundstein-dev /bin/bash",
    "test": "npm run test:cli",
    "format": "f -w"
  },
  "bin": {
    "grundstein": "src/bin.mjs"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
