  },
  "dependencies": {
    "@grundstein/commons": "github:grundstein/commons",
    "@magic/cli": "0.0.24",
    "@magic/error": "0.0.5",
    "@magic/fs": "0.0.5",
    "@magic/types": "0.1.11"
  },
  "devDependencies": {
    "@magic/format": "0.0.8",
    "@magic/test": "0.1.41"
  },
  "keywords": [
    "grundstein",
